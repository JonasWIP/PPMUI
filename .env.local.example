# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api
# Projects API (server-side only)
PROJECTS_API_URL=http://localhost:2020
# IMPORTANT: This token must be set for authentication with the Projects API
# For development, you can use 'dev-default-token' or get a proper token from your admin
PROJECTS_API_TOKEN=your-projects-api-token

# Authentication
NEXTAUTH_SECRET=your-nextauth-secret-at-least-32-chars
NEXTAUTH_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_FEATURE_AUTH=true
NEXT_PUBLIC_FEATURE_ANALYTICS=false

# Analytics (for production)
# NEXT_PUBLIC_ANALYTICS_ID=

# Deployment Environment
NODE_ENV=development
# NODE_ENV=production