{
  "name": "instancetest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:init": "npx supabase init --force",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:updatetypes": "npx supabase gen types typescript --db-url \"postgresql://postgres:postgres@localhost:54322/postgres\" > lib/database.types.ts",
    "supabase:dbpull": "npx supabase db pull",
    "supabase:dbreset": "npx supabase db reset",
    "supabase:dbpush": "npx supabase db push",
    "supabase:dbdiff": "npx supabase db diff",
    "supabase:dbsquash": "npx supabase db squash",
    "supabase:dbmigrationup": "npx supabase db migration up",
    "supabase:link": "npx supabase link",
    "supabase:dbseed": "npx supabase db seed"

  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "clsx": "^2.1.1",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
